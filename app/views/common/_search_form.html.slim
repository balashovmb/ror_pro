= form_tag search_path, method: :get do |f|
  .form-inline
    = search_field_tag :query, params[:query], placeholder: "Search", class: "form-control margin-5"
    = select_tag 'object', options_for_select(Search::OBJECTS, params[:object]) , class: "form-control margin-5"
    = submit_tag 'Find', class: "btn btn-primary margin-5"